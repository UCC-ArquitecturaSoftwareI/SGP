<% if(listaInscripciones && listaInscripciones.length > 0) {%>
<div class="alert alert-dark" role="alert" style="background-color: #ffffff">
  <h4 class="alert-heading"> <%= titulo %> </h4>
  <table id="tablaInscripciones" class="table table-striped table-hover" style="width: 50%; margin:auto;">
    <% if(listaInscripciones){ %>
    <thead>
    <tr>
      <th>Orden</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>DNI</th>
      <th></th>
    </tr>
    </thead>
    <tbody id="tablaEspera">

    <%
    for(var i = 0; i < listaInscripciones.length ; i++){ %>
    <tr>
      <td><%= i %></td>
      <td><%= listaInscripciones[i].persona.nombre %></td>
      <td><%= listaInscripciones[i].persona.apellido %></td>
      <td><%= listaInscripciones[i].persona.dni%></td>
      <td>
        <a href="/Alumno/<%= listaInscripciones[i].persona.id %>" class="btn-info btn">
          <i class="fas fa-eye"></i>
        </a>
      </td>
      <td>
        <button type="button" class="btn-danger btn" data-toggle="modal" data-target="#removeStudentModal"
                data-alumno-nombre="<%=listaInscripciones[i].persona.dni + ', ' + listaInscripciones[i].persona.apellido + ', ' + listaInscripciones[i].persona.nombre%>"
                data-alumno-id="<%=listaInscripciones[i].persona.id%>"
                data-curso-id="<%=listaInscripciones[i].curso.id%>"
                data-inscripcion-id="<%=listaInscripciones[i].id%>">
          <i class="fas fa-minus-circle"></i>
        </button>
      </td>
    </tr>
    <% } %>
    </tbody>
    <% } %>
  </table>
</div>

<% } else { %>
<div class="alert alert-success" role="alert">
  <h4 class="alert-heading"> <%= titulo %> </h4>
  <p><%= mensajeVacio1 %></p>
  <hr>
  <p class="mb-0"><%= mensajeVacio2 %></p>
</div>
<% } %>


