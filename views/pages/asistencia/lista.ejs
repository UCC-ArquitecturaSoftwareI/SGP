<h1>Listado de alumnos</h1>
<h3>Toma de asistencia</h3>

<table id="alumno" class="table table-striped table-hover" style="width: 65%; margin:auto;">
  <thead>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
  </tr>
  </thead>
  <tbody id="table">
  <% for(var i=0; i<pers.length; i++){ %>
  <tr onclick="setAs(<%= pers[i].id %>,<%= clase %>,<%= cursoTmp%>, this)">
    <td id="p1"><%= pers[i].nombre %></td>
    <td id="p1"><%= pers[i].apellido %></td>
  </tr>
  <% } %>
  </tbody>
</table>
