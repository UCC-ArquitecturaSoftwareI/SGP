<h1>Asistencia</h1>
<h5><%= persona.nombre %> <%= persona.apellido %></h5>

<table id="cursos" class="table table-striped table-hover" style="width: 50%; margin:auto;">
  <thread>
    <tr>
      <th>Clase</th>
      <th>Fecha</th>
      <th>Asistencia</th>
    </tr>
  </thread>
    <tbody id="table">
    <% for(var i = 0; i < alumno.length ; i++){ %>
    <tr>
      <td><%= alumno[i].clase %></td>
      <td><%= alumno[i].createdAt%></td>
      <td><button type="button" class="btn btn-outline-dark"><%= alumno[i].asistio%> </button></td>
    </tr>
    <% } %>
    </tbody>
</table>
