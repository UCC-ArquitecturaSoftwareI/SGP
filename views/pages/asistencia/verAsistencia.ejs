<h1>Listado de alumnos</h1>
<h3>Asistencia</h3>

<table id="alumno" class="table table-striped table-hover" style="width: 50%; margin:auto;">
  <thead>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Asistencias</th>
  </tr>
  </thead>
  <tbody id="table">
  <% for(var i = 0; i < pers.length ; i++){ %>
  <tr>
    <td><%= pers[i].nombre %></td>
    <td><%= pers[i].apellido %></td>
    <td><%= pers[i].asistencias%></td>
  </tr>
  <% } %>
  </tbody>
</table>
