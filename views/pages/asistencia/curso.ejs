<h1>Cursos</h1>

<table id="cursos" class="table table-striped table-hover" style="width: 50%; margin:auto;">
  <thread>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Info</th>
      <th>Nueva clase</th>
    </tr>
  </thread>

  <tbody id="tab">
  <% for(var i = 0; i < user.length ; i++){ %>
  <tr>
    <td><%= user[i].id %></td>
    <td><%= user[i].nombreDelCurso %></td>
    <td></td>
    <td></td>
    <td>
      <a href="/Alumno/<%= a[i].id %>" class="btn-info btn">
        <i class="fas fa-eye"></i>
      </a>
    </td>
    <td>
      <a href = "/Alumno/<%=a[i].id%>" class="custom-checkbox">
      </a>
    </td>
  </tr>
  <% } %>
  </tbody>
</table>
