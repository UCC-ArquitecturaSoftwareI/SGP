<form action="/buscarBautismo" method="get" style="width: 65%; margin:auto;">
  <div class="form-row">
    <label for="nombre">Buscar</label>
    <input type="text" name="nombre" onkeyup="buscarBautismo()">
    <a href="/bautismos/add" class="btn-info btn">Nuevo Turno</a>
  </div>
</form>

<table id="tablabautismos" class="table table-striped table-hover" style="width: 65%; margin:auto;">
  <thead>
  <tr>
    <th>Nombre</th>
    <th>DNI</th>
    <th>Fecha</th>
    <th>Turno</th>
    <th></th>
  </tr>
  </thead>
  <tbody id="tabla">
  <% for(var i = 0; i < a.length ; i++){
   var fecha = new Date(a[i].fecha*1); %>
  <tr>
    <td><%= a[i].nombre %></td>
    <td><%= a[i].dni %></td>
    <td><%= ("00"+fecha.getDate()).substr(-2) %>/<%= ("00"+(fecha.getMonth()+1)).substr(-2) %>/<%= fecha.getFullYear() %></td>
    <td><%= a[i].turno %></td>
    <td>
      <a href="/TurnoBautismo/<%= a[i].id %>" class="btn-info btn">
        <i class="fas fa-eye"></i>
      </a>
    </td>
  </tr>
  <% } %>
  </tbody>
</table>
