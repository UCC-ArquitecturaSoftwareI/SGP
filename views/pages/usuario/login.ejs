<div class="container">
  <div class="card card-login mx-auto mt-5">
    <div class="card-header">Ingreso</div>
    <div class="card-body">
      <form method="post" action="/login" >
        <div class="form-group">
          <label for="exampleInputEmail1">Correo</label>
          <input name="correo" class="form-control" id="exampleInputEmail1" type="email" aria-describedby="emailHelp" placeholder="Su correo electrónico">
        </div>
        <div class="form-group">
          <label for="exampleInputPassword1">Contraseña</label>
          <input name="contrasenia" class="form-control" id="exampleInputPassword1" type="password" placeholder="Su contraseña">
        </div>
        <input type="submit" class="btn btn-primary btn-block" value="Entrar">
      </form>
    </div>
  </div>
</div>
